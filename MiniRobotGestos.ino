void setup() {
  Serial.begin(9600);

  if (!APDS.begin()) {
    Serial.println("Error initializing APDS-9960 sensor!");
  }

  if(!display.begin(SSD1306_SWITCHCAPVCC, 0x3C)) {
    Serial.println(F("SSD1306 allocation failed"));
    for(;;);
  }

  display.clearDisplay();  // Limpia el buffer

  display.drawBitmap(0, 0, image_data_DISTRESSED_EYESarray, 128, 64, 1);
  display.display();
  delay(1000);

  servo1.attach(3);          // Asigna el sevomotor al pin 3 del Arduino 
  servo2.attach(4);
  servo1.write(detener);     // Aseguramos que el servo este detenido
  servo2.write(detener); 

}

void loop() {

  display.clearDisplay();  
  display.drawBitmap(0, 0, image_data_EYES_FRONTarray, 128, 64, 1);
  display.display();

  if (APDS.gestureAvailable()) {

    int gesture = APDS.readGesture();

    switch (gesture) {
      case GESTURE_UP:
        Serial.println("Detected UP gesture");
        servo1.write(giro2); 
        servo2.write(giro1);
        display.clearDisplay(); 
        display.drawBitmap(0, 0, image_data_EYES_DOWNarray, 128, 64, 1);
        display.display();
        delay(500);
        servo1.write(detener); 
        servo2.write(detener);
        break;

      case GESTURE_DOWN:
        Serial.println("Detected DOWN gesture");
        servo1.write(giro1); 
        servo2.write(giro2);
        display.clearDisplay(); 
        display.drawBitmap(0, 0, image_data_EYES_UParray, 128, 64, 1);
        display.display();
        delay(500);
        servo1.write(detener); 
        servo2.write(detener);
        break;

      case GESTURE_LEFT:
        Serial.println("Detected LEFT gesture");
        servo1.write(giro1); 
        display.clearDisplay(); 
        display.drawBitmap(0, 0, image_data_EYES_RIGHTarray, 128, 64, 1);
        display.display();
        delay(500);
        servo1.write(detener); 
        break;

      case GESTURE_RIGHT:
        Serial.println("Detected RIGHT gesture");
        servo2.write(giro2);
        display.clearDisplay(); 
        display.drawBitmap(0, 0, image_data_EYES_LEFTarray, 128, 64, 1);
        display.display();
        delay(500); 
        servo2.write(detener);
        break;

      default:
        break;
    }
  }
}
